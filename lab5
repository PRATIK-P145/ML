    "transactions = []\n",
    "for i in range(df.shape[0]):\n",
    "    transactions.append([\n",
    "        str(df.values[i, j]) \n",
    "        for j in range(df.shape[1]) \n",
    "        if str(df.values[i, j]) != \"nan\"\n",
    "    ])\n"
   ]
